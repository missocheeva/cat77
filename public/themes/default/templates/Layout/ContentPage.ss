<section class="content page-width-content">	
	<h1>$Title</h1>
	<% if $SubTitle %>
		<h2 class="subtitle">$SubTitle</h2>
	<% end_if %>	
	<article class="content">
			<header>
			$Content
			</header>
			<div class="left first">
				$LeftContent
			</div>	
			<div class="left">
				$RightContent
			</div>	
	</article>	
	<div class="clear"> </div>	
</section>